generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  user_id String @id @unique
  role    Int    @default(0)
  premium Int    @default(0)
}

model Guild {
  guild_id    String @id @unique
  channel_log String @unique
}
